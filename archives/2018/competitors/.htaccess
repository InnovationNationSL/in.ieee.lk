# i78 TECHNOLOGY PREMIUM RE-WRIRE RULES FOR USER FRIENDLY EXPERIENCE AND PERFECT SECURITY

RewriteBase /

RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d 
RewriteCond %{REQUEST_FILENAME}\.php -f 
RewriteRule ^(.*)$ $1.php


RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d 
RewriteCond %{REQUEST_FILENAME}\.html -f 
RewriteRule ^(.*)$ $1.html


